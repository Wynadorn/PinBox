{
    "configurations": [
        {
            "name": "3DS",
            "forcedInclude": ["${workspaceFolder}/include/vscode_fix.h"],
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include/**",
                "${DEVKITPRO}/devkitARM/arm-none-eabi/include/**",
                "${DEVKITPRO}/devkitARM/lib/gcc/arm-none-eabi/**",
                "${DEVKITPRO}/libctru/include/**",
                "${DEVKITPRO}/portlibs/3ds/include/**"
            ],
            "defines": [
                "_3DS",
                "ARM9",
                "ARM11",
                "_UNICODE"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/include/**",
                    "${DEVKITPRO}/devkitARM/arm-none-eabi/include/**",
                    "${DEVKITPRO}/devkitARM/lib/gcc/arm-none-eabi/**",
                    "${DEVKITPRO}/libctru/include/**",
                    "${DEVKITPRO}/portlibs/3ds/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "${DEVKITPRO}/devkitARM/bin/arm-none-eabi-g++"
        }
    ],
    "version": 4
}